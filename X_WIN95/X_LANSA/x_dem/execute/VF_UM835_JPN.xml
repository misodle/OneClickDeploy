<MENUITEM>
<PROPERTY NAME="CAPTION" VALUE="5250 ｻﾌﾞﾌｧｲﾙの処理" />
<PROPERTY NAME="HANDLER" VALUE="NEWLOOK" />
<SUBMENUS>
<SUBMENUITEM>
<PROPERTY NAME="CAPTION" VALUE="名前の付いたｶﾗﾑのSUBFILE_ACCESSORを使用" />
<PROPERTY NAME="STRING" VALUE= "{" />
<PROPERTY NAME="STRING" VALUE= "   /* ｻﾌﾞﾌｧｲﾙ ｱｸｾｽ ｵﾌﾞｼﾞｪｸﾄの作成 */" />
<PROPERTY NAME="STRING" VALUE= "   var objSFL = new SUBFILE_ACCESSOR(&quot;&lt;data grid control name&gt;&quot;);" />
<PROPERTY NAME="STRING" VALUE= "   var flagSelected = false;" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 選択ｶﾗﾑ名と選択ｵﾌﾟｼｮﾝを設定 */" />
<PROPERTY NAME="STRING" VALUE= "   objSFL.SetSelectionColumnName(&quot;&lt;column Name&gt;&quot;,&quot;&lt;selection Option Number&gt;&quot;);" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 検索ｶﾗﾑ名と検索ﾊﾞﾘｭｰ詳細を設定 */ " />
<PROPERTY NAME="STRING" VALUE= "   objSFL.SetSearchColumnName(&quot;&lt;column name&gt;&quot;,&quot;&lt;value to search for&gt;&quot;); " />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 選択項目の実行 */ " />
<PROPERTY NAME="STRING" VALUE= "   flagSelected = objSFL.SelectSubFileEntry(); " />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* ｱｸｾｽ ｵﾌﾞｼﾞｪｸﾄの処分 */ " />
<PROPERTY NAME="STRING" VALUE= "   objSFL.Dispose(); " />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 問題があった場合、ｴﾗｰﾒｯｾｰｼﾞで5250ﾌｫｰﾑを隠す */" />
<PROPERTY NAME="STRING" VALUE= "   if (!(flagSelected)) HIDE_CURRENT_FORM(&quot;XXXXXXXXXXX could not be displayed&quot;);" />
<PROPERTY NAME="STRING" VALUE= "}" />
<PROPERTY NAME="HANDLER" VALUE="NEWLOOK" />
</SUBMENUITEM>
<SUBMENUITEM>
<PROPERTY NAME="CAPTION" VALUE="番号の付いたｶﾗﾑのSUBFILE_ACCESSORを使用" />
<PROPERTY NAME="STRING" VALUE= "{" />
<PROPERTY NAME="STRING" VALUE= "   /* ｻﾌﾞﾌｧｲﾙ ｱｸｾｽ ｵﾌﾞｼﾞｪｸﾄの作成 */" />
<PROPERTY NAME="STRING" VALUE= "   var objSFL = new SUBFILE_ACCESSOR(&quot;&lt;data grid control name&gt;&quot;);" />
<PROPERTY NAME="STRING" VALUE= "   var flagSelected = false;" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 選択ｶﾗﾑ番号と選択ｵﾌﾟｼｮﾝを設定 */" />
<PROPERTY NAME="STRING" VALUE= "   objSFL.SetSelectionColumnNumber(&lt;column number&gt;,&quot;&lt;selection Option Number&gt;&quot;);" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 検索ｶﾗﾑ番号と検索ﾊﾞﾘｭｰ詳細を設定 */" />
<PROPERTY NAME="STRING" VALUE= "   objSFL.SetSearchColumnNumber(&lt;column number&gt;,&quot;&lt;value to search for&gt;&quot;);" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 選択項目の実行 */" />
<PROPERTY NAME="STRING" VALUE= "   flagSelected = objSFL.SelectSubFileEntry();" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* ｱｸｾｽ ｵﾌﾞｼﾞｪｸﾄの処分 */" />
<PROPERTY NAME="STRING" VALUE= "   objSFL.Dispose();" />
<PROPERTY NAME="STRING" VALUE= " " />
<PROPERTY NAME="STRING" VALUE= "   /* 問題があった場合、ｴﾗｰﾒｯｾｰｼﾞで5250ﾌｫｰﾑを隠す */" />
<PROPERTY NAME="STRING" VALUE= "   if (!(flagSelected)) HIDE_CURRENT_FORM(&quot;XXXXXXXXXXX could not be displayed&quot;);" />
<PROPERTY NAME="STRING" VALUE= "}" />
<PROPERTY NAME="HANDLER" VALUE="NEWLOOK" />
</SUBMENUITEM>
<SUBMENUITEM>
<PROPERTY NAME="CAPTION" VALUE="ｻﾌﾞﾌｧｲﾙ ｱｸｾｽを直接使用 - 基礎ｽｸﾘﾌﾟﾄ" />
<PROPERTY NAME="STRING" VALUE= "{" />  
<PROPERTY NAME="STRING" VALUE= "   var strDataGridName       = &quot;xxxxxxxxx&quot;; /* ﾃﾞｰﾀ ｸﾞﾘｯﾄﾞ名に変更 */" />
<PROPERTY NAME="STRING" VALUE= "   var flagAnotherPageExists = true;        /* 別のｻﾌﾞﾌｧｲﾙ ﾍﾟｰｼﾞが存在している */" />
<PROPERTY NAME="STRING" VALUE= "   do " />
<PROPERTY NAME="STRING" VALUE= "   {" />
<PROPERTY NAME="STRING" VALUE= "      var intColumnCount    = TONUMBER(GETVALUE(strDataGridName + &quot;.Columns.Count&quot;));" />   
<PROPERTY NAME="STRING" VALUE= "      var intRowCount       = TONUMBER(GETVALUE(strDataGridName + &quot;.RowCount&quot;));" />
<PROPERTY NAME="STRING" VALUE= "      var intColumn, intRow = 0;" />     
<PROPERTY NAME="STRING" VALUE= "      /* 現在のｻﾌﾞﾌｧｲﾙ ﾍﾟｰｼﾞを反復する */" /> 
<PROPERTY NAME="STRING" VALUE= "      for (intColumn = 0; intColumn &lt; intColumnCount; intColumn++) /* ｶﾗﾑを反復する */" />
<PROPERTY NAME="STRING" VALUE= "      {" />  
<PROPERTY NAME="STRING" VALUE= "         var strColumn     = intColumn.toString();" />  
<PROPERTY NAME="STRING" VALUE= "         var strColumnName = GETVALUE(strDataGridName + &quot;.Columns(&quot; + strColumn + &quot;).Name&quot;);" />   
<PROPERTY NAME="STRING" VALUE= "         TRACE(&quot;Column number &quot; + strColumn + &quot; is named \&quot;&quot; + strColumnName + &quot;\&quot;&quot;);" />  
<PROPERTY NAME="STRING" VALUE= "         for (intRow = 0; intRow &lt; intRowCount; intRow++) /* ｶﾗﾑの行を反復する */" />
<PROPERTY NAME="STRING" VALUE= "         {" /> 
<PROPERTY NAME="STRING" VALUE= "            var strRow          = intRow.toString();" />  
<PROPERTY NAME="STRING" VALUE= "            var strRowCellValue = GETVALUE(strDataGridName + &quot;.Columns(&quot; + strColumn + &quot;).Cells(&quot; + strRow + &quot;).Text&quot;);" />    
<PROPERTY NAME="STRING" VALUE= "            TRACE(&quot;   in row &quot; + strRow + &quot; it contains the value \&quot;&quot; + strRowCellValue + &quot;\&quot;&quot;);" />           
<PROPERTY NAME="STRING" VALUE= "         } /* ｶﾗﾑの行の反復を終了 */" /> 
<PROPERTY NAME="STRING" VALUE= "      } /* ｶﾗﾑの反復を終了 */" />
<PROPERTY NAME="STRING" VALUE= "      /* 指標に基づき次のｻﾌﾞﾌｧｲﾙ ﾍﾟｰｼﾞへ進む（進まない） */" />
<PROPERTY NAME="STRING" VALUE= "      flagAnotherPageExists = (GETVALUE(strDataGridName + &quot;.Marker&quot;) != &quot;&quot;);" />     
<PROPERTY NAME="STRING" VALUE= "      if (flagAnotherPageExists) { TRACE(&quot;Scrolling to next page.&quot;); SENDKEY(KeyPageDown); }" /> 
<PROPERTY NAME="STRING" VALUE= "      else                       { TRACE(&quot;End of subfile encountered&quot;); }" />  
<PROPERTY NAME="STRING" VALUE= "   } while(flagAnotherPageExists); /* 次のｻﾌﾞﾌｧｲﾙ ﾍﾟｰｼﾞへﾙｰﾌﾟし、処理を行う */" />  
<PROPERTY NAME="STRING" VALUE= "} /* 注記: このｽｸﾘﾌﾟﾄは、5250にｱｸｾｽするためのNewlookの使用に依存します。 */" />
<PROPERTY NAME="HANDLER" VALUE="NEWLOOK" />
</SUBMENUITEM>
</SUBMENUS>  
</MENUITEM>
