                    <allow address="*"/>
                    <allow address="10.2.1"/>
                    <allow address="10.2.1.45"/>

                    <deny useragent="*"/>
                    <deny useragent="?"/>
                    <deny useragent="edge"/>
                    <deny useragent="opera"/>
                    <deny useragent="chrome"/>
                    <deny useragent="safari"/>
                    <deny useragent="android"/>
                    <deny useragent="firefox"/>
                    <deny useragent="explorer"/>
